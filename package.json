{
	"name": "@ayako/website",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "rm -rf ./.svelte-kit && vite dev --port 5173",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview  --port 5173",
		"start": "rm -r ./.svelte-kit && pnpm build && pnpm preview --port 5173"
	},
	"dependencies": {
		"@formkit/auto-animate": "^0.8.2",
		"@iconify-json/tabler": "^1.2.17",
		"@sveltejs/adapter-cloudflare": "^7.0.1",
		"@unocss/extractor-svelte": "66.0.0",
		"@unocss/preset-icons": "^66.0.0",
		"@unocss/preset-rem-to-px": "^66.0.0",
		"@unocss/reset": "^66.0.0",
		"clsx": "^2.1.1",
		"discord-api-types": "0.37.120",
		"dotenv": "^16.5.0",
		"js-cookie": "^3.0.5",
		"tailwind-merge": "^3.2.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.14.1",
		"@typescript-eslint/eslint-plugin": "^8.30.1",
		"@typescript-eslint/parser": "^8.30.1",
		"@unocss/svelte-scoped": "^66.0.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "5.27.0",
		"typescript": "^5.8.3",
		"unocss": "^66.0.0",
		"unocss-preset-animations": "^1.1.1",
		"vite": "^6.2.6",
		"wrangler": "^4.11.0"
	},
	"packageManager": "pnpm@10.8.1",
	"engines": {
		"node": "^22.3.0",
		"pnpm": "^10.8.1"
	}
}
