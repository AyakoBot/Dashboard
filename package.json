{
	"name": "@ayako/dashboard",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "rm -rf ./.svelte-kit && vite dev --port 5173",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview  --port 5173",
		"start": "rm -r ./.svelte-kit && pnpm build && pnpm preview --port 5173"
	},
	"dependencies": {
		"@formkit/auto-animate": "^0.8.2",
		"@iconify-json/tabler": "^1.2.22",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@unocss/extractor-svelte": "66.4.2",
		"@unocss/preset-icons": "^66.4.2",
		"@unocss/preset-rem-to-px": "^66.4.2",
		"@unocss/reset": "^66.4.2",
		"clsx": "^2.1.1",
		"discord-api-types": "0.38.21",
		"dotenv": "^17.2.1",
		"js-cookie": "^3.0.5",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.1.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.34.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@types/eslint": "^9.6.1",
		"@types/js-cookie": "^3.0.6",

		"@types/node": "^24.3.0",
		"@typescript-eslint/eslint-plugin": "^8.41.0",
		"@typescript-eslint/parser": "^8.41.0",
		"@unocss/svelte-scoped": "^66.4.2",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "5.38.3",
		"svelte-eslint-parser": "^1.3.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0",
